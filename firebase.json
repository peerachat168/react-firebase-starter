{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
        "source": "/postresult/**",
        "function": "postresult"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
